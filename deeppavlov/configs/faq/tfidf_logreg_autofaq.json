{
  "dataset_reader": {
    "name": "faq_reader",
    "x_col_name": "Question",
    "y_col_name": "Answer",
    "data_url": "http://files.deeppavlov.ai/faq/school/faq_school.csv"
  },
  "dataset_iterator": {
    "name": "data_learning_iterator"
  },

  "chainer": {
    "in": "q",
    "pipe": [
      {
        "name": "ru_tokenizer",
        "in": "q",
        "lemmas": true,
        "out": "q_token_lemmas"
      },
      {
        "id": "vectorizer",
        "name": "tfidf_vectorizer",
        "in": "q_token_lemmas",
        "fit_on": ["q_token_lemmas"],
        "save_path": "vectorizer/tfidf_vectorizer.pkl",
        "load_path": "vectorizer/tfidf_vectorizer.pkl",
        "out": "q_vect"
      },
      {
        "name": "faq_logreg_model",
        "in": "q_vect",
        "fit_on": ["q_vect", "y"],
        "c": 1000,
        "penalty": "l2",
        "save_path": "faq/faq_tfidf_logreg_model.pkl",
        "load_path": "faq/faq_tfidf_logreg_model.pkl",
        "out": ["answer", "score"]
      }
    ],
    "out": ["answer", "score"]
  },

  "train": {
    "validate_best": false,
    "test_best": false
 },
  "metadata": {
    "download": [
      {
        "url": "http://files.deeppavlov.ai/faq/school/faq_tfidf_logreg_model.pkl",
        "subdir": "faq"
      },
      {
        "url": "http://files.deeppavlov.ai/faq/school/tfidf_vectorizer.pkl",
        "subdir": "vectorizer"
      }
    ]
  }

}



